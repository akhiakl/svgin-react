{
  "name": "svgin-react",
  "version": "0.1.0",
  "description": "Securely fetch and inline SVGs as React components.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./core": {
      "import": "./dist/core.js",
      "require": "./dist/core.js",
      "types": "./dist/core.d.ts"
    }
  },
  "peerDependencies": {
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^18.3.23",
    "dompurify": "^3.2.6",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.35.0",
    "vite": "^5.0.0"
  },
  "packageManager": "pnpm@10.12.3",
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm exec eslint . --ext .ts,.tsx"
  },
  "keywords": [
    "react",
    "svg",
    "inline",
    "component",
    "sanitize",
    "dompurify"
  ],
  "author": "",
  "license": "MIT"
}
