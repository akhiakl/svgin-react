{
  "name": "svgin-react",
  "version": "0.5.1",
  "description": "Securely fetch and inline SVGs as React components.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/client.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "react-server": "./dist/server.js",
      "react-client": "./dist/client.js",
      "import": "./dist/client.js",
      "require": "./dist/client.js"
    },
    "./client": {
      "import": "./dist/client.js",
      "require": "./dist/client.js"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.js"
    },
    "./core": {
      "import": "./dist/core.js",
      "require": "./dist/core.js"
    }
  },
  "peerDependencies": {
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^18.3.23",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "isomorphic-dompurify": "^2.26.0",
    "jiti": "^2.5.1",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.0.0"
  },
  "packageManager": "pnpm@10.12.3",
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm exec eslint . --ext .ts,.tsx"
  },
  "keywords": [
    "react",
    "svgin-react",
    "react-svgin",
    "svgin",
    "inline-svg",
    "inline svg",
    "inline react svg",
    "svg",
    "inline",
    "component",
    "sanitize",
    "dompurify"
  ],
  "author": "",
  "license": "MIT"
}